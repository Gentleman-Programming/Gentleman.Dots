[
  {
    "label": "Lazygit",
    "command": "lazygit -p $ZED_WORKTREE_ROOT",
    "use_new_terminal": true,
    "allow_concurrent_runs": false,
    "reveal": "always",
    "hide": "on_success",
    "target": "center"
  },
  {
    "label": "Oil",
    "command": "oil-zed $ZED_WORKTREE_ROOT",
    "use_new_terminal": true,
    "allow_concurrent_runs": false,
    "reveal": "always",
    "hide": "on_success",
    "target": "center"
  },
  {
    "label": "Oil Float",
    "command": "oil-float $ZED_WORKTREE_ROOT",
    "use_new_terminal": true,
    "allow_concurrent_runs": false,
    "reveal": "always",
    "hide": "on_success",
    "target": "center"
  },
  {
    "label": "Oil Simple",
    "command": "oil-simple $ZED_WORKTREE_ROOT",
    "use_new_terminal": true,
    "allow_concurrent_runs": true,
    "reveal": "always",
    "hide": "on_success"
  },
  {
    "label": "Yazi",
    "command": "ya_zed $ZED_FILE",
    "use_new_terminal": true,
    "allow_concurrent_runs": false,
    "reveal": "always",
    "hide": "on_success",
    "target": "center"
  },
  {
    "label": "File Finder",
    "command": "zed --add \"$(tv files)\"",
    "hide": "always",
    "allow_concurrent_runs": true,
    "use_new_terminal": true
  },
  {
    "label": "Find in Files",
    "command": "zed --add \"$(tv text)\"",
    "hide": "always",
    "allow_concurrent_runs": true,
    "use_new_terminal": true
  },
  {
    "label": "Find Directories",
    "command": "zed --add \"$(tv dirs)\"",
    "hide": "always",
    "allow_concurrent_runs": true,
    "use_new_terminal": true
  },
  {
    "label": "Obsidian",
    "command": "cd ~/.config/obsidian && zed --add \"$(tv files)\"",
    "hide": "always",
    "allow_concurrent_runs": false,
    "use_new_terminal": true
  }
]
